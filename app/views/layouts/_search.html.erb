<div class="search-form">
  <%= form_with url: search_path, method: :get, local: true do |f| %>
    <%= f.text_field :content %>
    <%= f.select :model, options_for_select({ "User" => "user", "Wardrobe" => "wardrobe", "Image" => "post_image", "Tag" => "tag", "Event" => "event" }) %>
    <%= f.select :method, options_for_select({ "Parfect" => "perfect", "Forward" => "forward", "Backward" => "backward", "Partial" => "partial" }) %>
    <%= f.submit '検索' %>
  <% end %>
</div>